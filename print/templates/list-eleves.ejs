<!DOCTYPE html>
<html>

<head>
    <!-- inject:css -->
    <link rel="stylesheet" href="/css/print.css">
    <title> <%=pageTitle%></title>
</head>

<body onload="window.print()">
    <%- include("header") -%>
        <div class="row-header-table">
            <div class="title-report">
                <%=pageTitle%><br>
                Semaine du _________ au ________ Mois _________________ Année ____________
                <br>
            </div>
        </div>
        </div>
        <div class=" row-report">
            <div class="col-sm-12">
                    <table class="table-list">
                        <thead>
                            <tr>
                                <th class="th-list-student">#</th>
                                <th class="th-list-student">Elèves/Etudiants</th>
                                <th class="th-list-student">Sexe</th>
                                <th class="th-list-student">Lundi</th>
                                <th class="th-list-student">Mardi</th>
                                <th class="th-list-student">Mercredi</th>
                                <th class="th-list-student">Jeudi</th>
                                <th class="th-list-student">Vendredi</th>
                                <th class="th-list-student">Remarques</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% let pos=1; studentList.forEach((Student)=> { %>
                                <tr>
                                    <td align="center" class="td-list-student"><%=pos%></td>
                                    <td class="td-fullname-list-student">
                                        <% if(ClassRoom=="All"){ %>
                                            <%=Student.nom.toUpperCase()+" "+Student.prenom%>&nbsp;
                                            <strong><%=Student.abv%></strong>
                                        <% }else{ %>
                                            <%=Student.nom.toUpperCase()+" "+Student.prenom%>
                                        <% } %>
                                    </td>
                                    <td class="td-list-student" align="center"><%=Student.sexe%></td>
                                    <td class="td-list"></td>
                                    <td class="td-list"></td>
                                    <td class="td-list"></td>
                                    <td class="td-list"></td>
                                    <td class="td-list"></td>
                                    <td class="td-fullname"></td>
                                </tr>
                                <% pos++; }) %>
                        </tbody>
                    </table>
            </div>
        </div>

</body>

</html>